{
  "name": "@scorocode/server-sdk",
  "version": "1.5.0",
  "description": "Scorocode v2 JS Server SDK",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "./dist/cjs/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/Scorocode/sc-v2-js-server-sdk"
  },
  "author": "Scorocode <scorocode@gmail.com> (https://scorocode.ru)",
  "license": "Apache-2.0",
  "homepage": "https://scorocode.ru",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "cleanup": "shx rm -rf dist types coverage docs package",
    "prebuild": "npm run cleanup",
    "build": "gulp build",
    "bridge": "sc-cli bridge -dir=app",
    "docs": "typedoc -p . --theme minimal --target 'es6' --excludeNotExported --excludePrivate --ignoreCompilerErrors --exclude \"**/src/**/__tests__/*.*\" --out docs src/",
    "test": "jest -c ./config/jest.config.js",
    "test:watch": "npm t -- --watch",
    "test:coverage": "npm t -- --coverage",
    "test:ci": "npm t -- --ci",
    "verify": "npm run style && npm run test:ci",
    "commit": "git-cz",
    "style": "npm run format -- --list-different && npm run lint",
    "style:fix": "npm run format:fix && npm run lint:fix",
    "format": "prettier --config config/prettier.config.js \"**/*.{ts,tsx,js,jsx,css,scss,sass,less,md}\"",
    "format:fix": "npm run format -- --write",
    "lint": "tslint --project tsconfig.json --format codeFrame",
    "lint:fix": "npm run lint -- --fix",
    "prerelease": "run-p -r bridge verify",
    "release": "standard-version",
    "postrelease": "npm run build && npm run release:github && npm run release:npm",
    "release:github": "git push --no-verify --follow-tags origin master",
    "release:npm": "npm publish",
    "release:preflight": "npm pack"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "maxSubjectLength": 120
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": "lint-staged",
      "pre-push": "npm run style && npm test -- --bail --onlyChanged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx,css,scss,sass,less,md}": [
      "prettier --config config/prettier.config.js --write",
      "git add"
    ],
    "src/**/*.{ts,tsx}": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "standard-version": {
    "skip": {
      "changelog": true
    }
  },
  "peerDependencies": {
    "google-protobuf": ">=3.6.1",
    "grpc": ">=1.16.0",
    "tslib": ">=1.9.0",
    "typescript": ">=2.9.0"
  },
  "dependencies": {
    "bson": "^3.0.2",
    "grpc": "1.21.1",
    "lodash": "4.17.10"
  },
  "devDependencies": {
    "@types/bson": "1.0.11",
    "@types/deep-extend": "0.4.31",
    "@types/google-protobuf": "3.2.7",
    "@types/jest": "23.0.2",
    "@types/node": "10.3.2",
    "@types/prettier": "1.13.1",
    "@types/protobufjs": "6.0.0",
    "@types/ws": "6.0.1",
    "awesome-typescript-loader": "5.0.0",
    "commitizen": "2.10.1",
    "cross-env": "5.2.0",
    "cross-var": "1.1.0",
    "cz-conventional-changelog": "2.1.0",
    "download-file": "^0.1.5",
    "google-protobuf": "3.8.0",
    "gulp": "^4.0.0",
    "gulp-clean": "0.4.0",
    "gulp-replace": "1.0.0",
    "gulp-sourcemaps": "2.6.4",
    "gulp-typescript": "5.0.0-alpha.3",
    "gzip-size-cli": "2.1.0",
    "husky": "1.0.0-rc.8",
    "jest": "23.6.0",
    "lint-staged": "8.1.0",
    "merge2": "1.2.3",
    "npm-run-all": "4.1.5",
    "prettier": "1.15.3",
    "shelljs": "^0.8.2",
    "shx": "0.3.0",
    "standard-version": "4.4.0",
    "strip-json-comments-cli": "1.0.1",
    "ts-jest": "22.4.6",
    "tslib": "1.9.2",
    "tslint": "5.10.0",
    "tslint-config-prettier": "1.13.0",
    "tslint-config-standard": "7.0.0",
    "tslint-react": "3.6.0",
    "typedoc": "0.11.1",
    "typescript": "2.9.1",
    "validate-commit-msg": "2.14.0",
    "ws": "6.1.2"
  }
}
